        {
          "name": "用户登录",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "id": "4fd397d1-692d-46b5-889d-06d08d49a14d",
                "type": "text/javascript",
                "exec": [
                  "var params = JSON.parse(request.data).params;",
                  "var raw_phone = params['raw_phone'];",
                  "var raw_password = params['raw_password'];",
                  "",
                  "console.log(params)",
                  "var username = raw_phone;",
                  "",
                  "pm.sendRequest({",
                  "    url: pm.environment.get('host') + '/' + pm.environment.get('project') + '/v1/sys/tools/encrypt',",
                  "    method: \"POST\",",
                  "    header: {",
                  "        'Content-Type': 'application/json'",
                  "    },",
                  "    body: {",
                  "        mode: 'raw',",
                  "        raw: JSON.stringify({",
                  "            params: {",
                  "                \"isPub\": true, ",
                  "                \"encryptStr\": raw_password",
                  "            }",
                  "        })",
                  "    }",
                  "}, function (err, res) {",
                  "    var result = res.json().result;",
                  "    var encryptResult = result.encryptResult;",
                  "    // 加密前消息",
                  "    console.log('加密前：', raw_password);",
                  "    // 加密后消息",
                  "    console.log('密文：', encryptResult);",
                  "    ",
                  "    // 设置变量",
                  "    postman.setEnvironmentVariable('phone', username);",
                  "    postman.setEnvironmentVariable('password', encryptResult);",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n\t\"params\": {\n\t\t\"raw_phone\": \"13819493700\",\n\t    \"raw_password\": \"493700\",\n\t    \"phone\": \"{{phone}}\",\n\t    \"password\": \"{{password}}\"\n\t}\n}"
