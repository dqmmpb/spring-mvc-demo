        {
          "name": "用户修改密码",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "id": "0f4cce32-cb57-47c1-8123-2adec92498fa",
                "type": "text/javascript",
                "exec": [
                  "var params = JSON.parse(request.data).params;",
                  "var raw_oldPwd = params['raw_oldPwd'];",
                  "var raw_newPwd = params['raw_newPwd'];",
                  "",
                  "console.log(params)",
                  "",
                  "pm.sendRequest({",
                  "    url: pm.environment.get('host') + '/' + pm.environment.get('project') + '/v1/sys/tools/encrypt',",
                  "    method: \"POST\",",
                  "    header: {",
                  "        'Content-Type': 'application/json'",
                  "    },",
                  "    body: {",
                  "        mode: 'raw',",
                  "        raw: JSON.stringify({",
                  "            params: {",
                  "                \"isPub\": true, ",
                  "                \"encryptStr\": raw_oldPwd",
                  "            }",
                  "        })",
                  "    }",
                  "}, function (err, res) {",
                  "    var result = res.json().result;",
                  "    var encryptResult = result.encryptResult;",
                  "    // 加密前消息",
                  "    console.log('加密前：', raw_oldPwd);",
                  "    // 加密后消息",
                  "    console.log('密文：', encryptResult);",
                  "    ",
                  "    // 设置变量",
                  "    postman.setEnvironmentVariable('oldPwd', encryptResult);",
                  "    ",
                  "    pm.sendRequest({",
                  "        url: pm.environment.get('host') + '/' + pm.environment.get('project') + '/v1/sys/tools/encrypt',",
                  "        method: \"POST\",",
                  "        header: {",
                  "            'Content-Type': 'application/json'",
                  "        },",
                  "        body: {",
                  "            mode: 'raw',",
                  "            raw: JSON.stringify({",
                  "                params: {",
                  "                    \"isPub\": true, ",
                  "                    \"encryptStr\": raw_newPwd",
                  "                }",
                  "            })",
                  "        }",
                  "    }, function (err, res) {",
                  "        var result = res.json().result;",
                  "        var encryptResult = result.encryptResult;",
                  "        // 加密前消息",
                  "        console.log('加密前：', raw_newPwd);",
                  "        // 加密后消息",
                  "        console.log('密文：', encryptResult);",
                  "        ",
                  "        // 设置变量",
                  "        postman.setEnvironmentVariable('newPwd', encryptResult);",
                  "    });",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
